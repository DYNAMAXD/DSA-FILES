
def isPathCrossing(self, path):
    """
    :type path: str
    :rtype: bool
    """
    x = 0
    y = 0
    for i in path:
        if i == "N":
            y+=1
        elif i == "S":
            y-=1
        elif i == "E":
            x+=1
        elif i == "W":
            x-=1
        if (x , y) == (0,0):
            return True
            if x == y:
                return True
    return False
    